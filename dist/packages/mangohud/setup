#!/usr/bin/env bash

set -euEo pipefail
exit 0
## Download & Extract
cd "$(mktemp -d)"
wget -qO - https://api.github.com/repos/flightlessmango/MangoHud/releases/latest |
  grep -Pom 1 'https://.*MangoHud-v\d+\.\d+\.\d+\.tar\.gz' |
  wget -qO - -i - |
  tar xzf -

# Install
cd ./MangoHud
./mangohud-setup.sh install
